<Page x:Class="MSBeverageRecordApp.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MSBeverageRecordApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">


    <Grid>

        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File">
                <MenuItem x:Name="muiSave" Header="_Save" Click="muiSave_Click"/>
            </MenuItem>
        </Menu>
        <DataGrid x:Name="MSBeverageRecordApp" Margin="51,97,51,205"/>

        <Label  FontSize="22px" Content="Mississippi Beverage Records" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="47" Width="300"/>
        <TextBox x:Name="consoleOutput" HorizontalAlignment="Left" Margin="182,296,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>

        <ComboBox x:Name="Filter" Margin="51,56,609,337" SelectionChanged="Filter_SelectionChanged">
        </ComboBox>
    </Grid>
</Page>
